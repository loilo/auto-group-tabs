{
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#00bc72",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#00bc7299",
    "titleBar.inactiveForeground": "#e7e7e799",
    "sash.hoverBorder": "#00ef91",
    "commandCenter.border": "#e7e7e799"
  },
  "peacock.color": "#00bc72",
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/extension": true
  },
  "path-intellisense.extensionOnImport": false,
  "files.watcherExclude": {
    "**/extension/**": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "vue.suggest.componentNameCasing": "preferPascalCase"
}
